int sumSquares = 0;
int squareSum = 0;
int[] numbers;

void setup(){
  numbers = new int[101];
  for (int i = 1; i<101; i++){
    numbers[i] = i;
  }
  for  (int i = 1; i<101; i++){
    sumSquares += i*i;
    squareSum += i;
  }
  squareSum = squareSum*squareSum;
  println(squareSum - sumSquares);
}
